<workflow-app name="Sqoop" xmlns="uri:oozie:workflow:0.4">
    <start to="Export"/>
    <action name="Export">
        <sqoop xmlns="uri:oozie:sqoop-action:0.2">
            <job-tracker>${jobTracker}</job-tracker>
            <name-node>${nameNode}</name-node>
              <job-xml>${nameNode}/tmp/Lab8/Task2/hive-site.xml</job-xml>
            <command>export --connect jdbc:mysql://10.128.0.3:3306/nums --username root -m 1 --table Nums --hcatalog-database oozie --hcatalog-table nums </command>
        </sqoop>
        <ok to="end"/>
        <error to="kill"/>
    </action>
    <kill name="kill">
        <message>Action failed, error message[${wf:errorMessage(wf:lastErrorNode())}]</message>
    </kill>
    <end name="end"/>
</workflow-app>
